# Combine tables

CREATE TABLE table_divvy_year AS
  (SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM 
    202102_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202103_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202104_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202105_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202106_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202107_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202108_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202109_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202110_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202111_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202112_divvy_tripdata
  UNION
  SELECT
    ride_id, rideable_type, ride_length, day_of_week_name, start_station_name, end_station_name, member_casual
  FROM
    202201_divvy_tripdata);
  
  # My SQL Workbnech
  # Don't Limit
  # Edit > Preferences > SQL Editor > DBMS connection read timeout internal (in seconds) > 6000
  # Change C:\ProgramData\MySQL\MySQL Server 8.0\my.ini buffer_pool_size to 4G
